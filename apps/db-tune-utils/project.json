{
  "name": "db-tune-utils",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/db-tune-utils/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "development",
      "options": {
        "outputFileName": "db-tune.js",
        "thirdParty": true,
        "platform": "node",
        "outputPath": "dist/apps/db-tune-utils",
        "format": ["cjs"],
        "bundle": true,
        "main": "apps/db-tune-utils/src/main.ts",
        "tsConfig": "apps/db-tune-utils/tsconfig.app.json",
        "externalDependencies": true,
        "generatePackageJson": false,
        "esbuildConfig": "environments/esbuild.config.ts"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "production",
      "dependsOn": ["build"],
      "options": {
        "buildTarget": "db-tune-utils:build",
        "runBuildTargetDependencies": true
      },
      "configurations": {
        "development": {
          "buildTarget": "db-tune-utils:build:development"
        },
        "production": {
          "buildTarget": "db-tune-utils:build:production"
        }
      }
    },
    "test": {
      "options": {
        "passWithNoTests": true
      }
    }
  }
}
